<div class="main">
 
    
    
     
      
         {{#each match}}
          <a class=" grid-matches"  href="/matches/match/{{this._id}}/{{this.teams.[1]._id}}/{{this.disciplines._id}}">
             <div ><img  src="/img/{{this.disciplines.img}}" alt=" logo" width="30" height="30"></div>
             <div>{{this.date}}</div><div>{{this.teams.[0].organization.name}}</div>
             <div><img  src="/img/{{this.teams.[0].organization.img}}" alt=" logo" width="45" height="45"></div>
             <div style="font-weight: bold;">{{this.score.[0]}}:{{this.score.[1]}}</div>
             <div><img  src="/img/{{this.teams.[1].organization.img}}" alt=" logo" width="45" height="45"></div>
             <div>{{this.teams.[1].organization.name}}</div>
             <div>{{this.name}}</div>
          </a> 
        {{/each}}
        <p><a href="/matches">To matches</a></p>
     </div>